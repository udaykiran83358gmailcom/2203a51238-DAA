col= int(input(""))
a=[[0]*col]*rows
b=[[0]*col]*rows
for i in range(rows):
  for j in range(col):
    a[i][j]=int(input(""))
for i in range(rows):
  for j in range(col):
    b[i][j]=int(input(""))
pro=[[0]*col]*rows

for i in range(rows):
  for j in range(col):
    pro[i][j]+=a[i][j]*b[j][i]
for i in range(rows):
  for j in range(col):
    print(pro[i][j],end=" ")
  print()
     
